{
  "name": "fastify-box",
  "description": "Web application template based on fastify, with customized extensions supported by the community.",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "ncu -u && npm i"
  },
  "keywords": [
    "fastify-box",
    "framework",
    "template",
    "fastify",
    "box",
    "web"
  ],
  "author": "goodvir",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/goodvir/fastify-box.git"
  },
  "bugs": {
    "url": "https://github.com/goodvir/fastify-box/issues"
  },
  "homepage": "https://github.com/goodvir/fastify-box#readme",
  "engines": {
    "node": ">=10"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "fastify": "^3.2.0",
    "fastify-accepts": "^2.0.1",
    "fastify-autoload": "^3.0.7",
    "fastify-compress": "^3.3.0",
    "fastify-cors": "^4.1.0",
    "fastify-helmet": "^5.0.0",
    "fastify-multipart": "^2.0.3",
    "fastify-plugin": "^2.3.0",
    "fastify-rate-limit": "^4.0.2",
    "fastify-request-context": "^2.0.0",
    "fastify-sensible": "^3.0.1",
    "fastify-static": "^3.2.0",
    "html-minifier": "^4.0.0",
    "hyperid": "^2.0.5",
    "make-promises-safe": "^5.1.0",
    "point-of-view": "^4.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "npm-check-updates": "^7.1.0",
    "pino-pretty": "^4.1.0",
    "prettier": "2.0.5"
  },
  "nodemonConfig": {
    "ext": "js",
    "watch": [
      "core",
      "app.js",
      "config.js",
      ".env"
    ]
  }
}
