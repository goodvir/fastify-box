  "author": "Dmitrii Shchipunov",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "fastify": "^3.1.1",
    "fastify-accepts": "^2.0.1",
    "fastify-autoload": "^3.0.6",
    "fastify-compress": "^3.2.2",
    "fastify-cors": "^4.1.0",
    "fastify-helmet": "^4.0.2",
    "fastify-plugin": "^2.0.3",
    "fastify-rate-limit": "^4.0.2",
    "fastify-request-context": "^1.0.2",
    "fastify-sensible": "^3.0.1",
    "fastify-static": "^3.2.0",
    "html-minifier": "^4.0.0",
    "hyperid": "^2.0.5",
    "make-promises-safe": "^5.1.0",
    "point-of-view": "^4.2.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.4",
    "npm-check-updates": "^7.0.2",
    "pino-pretty": "^4.1.0",
    "prettier": "2.0.5"
  },
  "nodemonConfig": {
    "ext": "js",
    "watch": [
      "core",
      "app.js",
      "config.js",
      ".env"
