{
  "name": "fastify-box",
  "version": "3.0.0",
  "description": "Fastify app template",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update": "ncu -u && npm i"
  },
  "keywords": [
    "framework",
    "template",
    "fastify",
    "web"
